# 예제 7-1
CREATE TABLE A (
    ID VARCHER(20) NOT NULL,
    NAME VARCHER(20) NOT NULL,
    AGE INT,
    LEVEL VARCHER(10) NOT NULL,
    JOP VARCHER(20),
    POINT INT DEFAULT 0,
    PRIMARY KEY(ID)
);

# 7-2
CREATE TABLE PRODUCT (
    NUM CHAR(3) NOT NULL,
    P_NAME VARCHER(20),
    QU  INT,
    ONE INT,
    COMP VARCHER(2),
    PRIMART KEY(NUM),
    CHECK (QU >= AND QU <=10000)
);

# 7-3
CREATE TABLE ORDER(
    NUM CHAR(3) NOT NULL,
    COM VARCHER(20),
    PRO CHAR(3),
    QU INT,
    ADR VARCHER(30),
    DAY DATE,
    PRIMARY KEY(NUM),
    FOREIGN KEY(COM) PEFERENCES COM(ID),
    FOREIGN KEY(PRO) PEFERENCES PRO(P_NUM)
);